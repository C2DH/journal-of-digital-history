{
  "name": "website",
  "private": true,
  "version": "6.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build --config vite.config.js",
    "analyze": "source-map-explorer 'dist/assets/*.js'",
    "build-noindex": "vite build && echo 'User-agent: *\nDisallow: /' > dist/robots.txt && echo '/* /index.html  200' > dist/_redirects && cp -r src/data/mock-api dist",
    "lint": "eslint --fix src/**/*.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test-storybook": "test-storybook",
    "test-storybook:ci": "npx concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"npx http-server storybook-static --port 6006 --silent\" \"npx wait-on tcp:6006 && yarn test-storybook\""
  },
  "dependencies": {
